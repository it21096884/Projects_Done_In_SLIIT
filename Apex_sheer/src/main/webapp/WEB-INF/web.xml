<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
 version="3.1">
 
 
 <servlet>
	 <servlet-name>SalMainPage</servlet-name>
	 <servlet-class>com.Salary.salaryservlet</servlet-class>
 </servlet>
 
 <servlet-mapping>
	 <servlet-name>SalMainPage</servlet-name>
	 <url-pattern>/SalMainPage</url-pattern>
 </servlet-mapping>
 
 
 
 
 <servlet>
	 <servlet-name>ResignPage</servlet-name>
	 <servlet-class>com.Salary.deleteempservlet</servlet-class>
 </servlet>
 
 <servlet-mapping>
	 <servlet-name>ResignPage</servlet-name>
	 <url-pattern>/ResignPage</url-pattern>
 </servlet-mapping>
 
 
 
 
 <servlet>
 <servlet-name>UpdatePayment</servlet-name>
 <servlet-class>com.Salary.UpdateAmount</servlet-class>
 </servlet>
 
 <servlet-mapping>
 <servlet-name>UpdatePayment</servlet-name>
 <url-pattern>/UpdatePayment</url-pattern>
 </servlet-mapping>
 
 
 
 
 
 <servlet>
 <servlet-name>RegisterEmployee</servlet-name>
 <servlet-class>com.Salary.InsertEmp</servlet-class>
 </servlet>
 
 <servlet-mapping>
 <servlet-name>RegisterEmployee</servlet-name>
 <url-pattern>/RegisterEmployee</url-pattern>
 </servlet-mapping>
 

 </web-app>